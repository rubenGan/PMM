package com.ruben.trabajo;

public class Titular 
{
	private String titulo;
	private String subtitulo;
	private String edad;

	public Titular(String tit, String sub){
		titulo = tit;
		subtitulo = sub;
	}
	
	public String getTitulo(){
		return titulo;
	}
	
	public String getSubtitulo(){
		return subtitulo;
	}
	
	public String getEdad(){
		return edad;
	}
}
